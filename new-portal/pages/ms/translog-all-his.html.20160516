<!doctype html>
<html><head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="renderer" content="webkit">
    <title>历史交易流水</title>
    <link rel="stylesheet" type="text/css" href="../../js/themes/default/xui.css">
    <link rel="stylesheet" type="text/css" href="../../js/themes/icon.css">
    <link rel="stylesheet" type="text/css" href="../../css/tool.css">
    <script type="text/javascript" src="../../js/jquery.min.js"></script>
    <script type="text/javascript" src="../../js/jquery.xui.min.js"></script>
    <script type="text/javascript" src="../../js/My97DatePicker/WdatePicker.js"></script><link href="../../js/My97DatePicker/skin/WdatePicker.css" rel="stylesheet" type="text/css">
    <script type="text/javascript" src="../../js/jquery.xconfig.js"></script>
    <script type="text/javascript" src="../../js/jquery.tool.js"></script>
</head>

<body class="search-body" style="padding:5px;">
	<div class="search-div">
    <div>
        <form id="search-fm" class="form search-form" action="/action/ms/translog-all-his/search" method="post">
            <table class="table" style="width:950px;">
                <caption>查询历史交易流水</caption>
                <tbody><tr>
        <td class="right td20"><label>清算日期：</label></td>
        <td class="td30">
        	<input id="settle_date" type="text" class="Wdate  xui-validatebox" name="settle_date" data-options="required:true" style="width:100px;" onfocus="WdatePicker({onpicked:function(){$dp.$('settle_date2').focus();},maxDate:'#F{$dp.$D(\'settle_date2\')}'})">
            <input id="settle_date2" type="text" class="Wdate xui-validatebox" name="settle_date2" data-options="required:true" style="width:100px;" onfocus="WdatePicker({minDate:'#F{$dp.$D(\'settle_date\')}'})"><label class="required">*</label>
        	</td>
        <td class="right td20"><label>客户ID：</label></td>
        <td class="td30"><input type="text" name="mcht_id"></td>
                </tr>
                <tr>
                  <td class="right td20"><label>终端标识码：</label></td>
                  <td class="td30"><input type="text" name="teminal_id"></td>
                  <td class="right td20"><label>PSAM卡号：</label></td>
                  <td class="td30"><input type="text" name="psam_no"></td>
                </tr>
                <tr>
                  <td class="right td20"><label>商户编码：</label></td>
                  <td class="td30"><input type="text" name="shop_id"></td>
                  <td class="right td20"><label>终端编码：</label></td>
                  <td class="td30"><input type="text" name="pos_id"></td>
                </tr>
              <tr>
                <td class="right td20"><label>交易卡号：</label></td>
                <td class="td30"><input type="text" name="card_no"></td>
                <td class="right td20"><label>转入账号：</label></td>
                <td class="td30"><input type="text" name="in_pan"></td>
              </tr>
                </tbody><tfoot>
                    <tr>
                        <td colspan="4" class="center">
                            <div style="margin:5px;"></div>
                            <a href="javascript:void(0)" class="xui-linkbutton l-btn" data-options="iconCls:'icon-search',datagrid:'#search-dg',onBefore:onBeforeSearch" style="padding:0 5px;" onclick="xTool.search(this)"><span class="l-btn-left l-btn-icon-left"><span class="l-btn-text">查询</span><span class="l-btn-icon icon-search">&nbsp;</span></span></a>
                            &nbsp;&nbsp;
                            <a href="javascript:void(0)" class="xui-linkbutton l-btn" data-options="iconCls:'icon-clear',datagrid:'#search-dg'" style="padding:0 5px;" onclick="xTool.reset(this)"><span class="l-btn-left l-btn-icon-left"><span class="l-btn-text">重置</span><span class="l-btn-icon icon-clear">&nbsp;</span></span></a>
                            &nbsp;&nbsp;
              				<a href="javascript:void(0)" class="xui-linkbutton l-btn" data-options="iconCls:'icon-export',url:'/action/ms/translog-all-his/export'" style="padding:0 5px;" onclick="xTool.exportData(this)"><span class="l-btn-left l-btn-icon-left"><span class="l-btn-text">导出</span><span class="l-btn-icon icon-export">&nbsp;</span></span></a>
                        </td>
                    </tr>
                </tfoot>
            </table>
        </form>
    </div>
    <br>
    <div class="xui-datagrid datagrid" style="min-width: 800px;" id="search-dg" data-options="pagination:true"><div class="datagrid-header">历史交易流水</div><div class="datagrid-body"><div class="datagrid-view"><table class="datagrid-table" border="0" cellspacing="0" cellpadding="0"><thead><tr class="datagrid-header-row"><th class="datagrid-header-rownumber" data-options="field:'rn',rownumber:true"></th><th data-options="field:'settle_date',width:80">清算日期</th><th data-options="field:'date_time',width:100">交易时间</th><th data-options="field:'card_no',width:200">交易卡号</th><th data-options="field:'trans_type',width:80">交易类型</th><th data-options="field:'amount',width:80,formatter:formatAmount">交易金额</th><th data-options="field:'trans_count',width:80">交易笔数</th><th data-options="field:'in_pan',width:150">转入账号</th><th data-options="field:'fee',width:80,formatter:formatAmount">商户手续费</th><th data-options="field:'tl_fee',width:80,formatter:formatAmount">通联收益</th><th data-options="field:'brand_fee',width:80,formatter:formatAmount">品牌费</th><th data-options="field:'acq_id',width:80">收单机构</th><th data-options="field:'psam_no',width:120">PSAM卡号</th><th data-options="field:'shop_id',width:120">商户编码</th><th data-options="field:'pos_id',width:80">终端编码</th><th data-options="field:'serial_number',width:80">系统跟踪号</th><th data-options="field:'authorize_code',width:80">授权码</th><th data-options="field:'issuer_id',width:80">发卡机构代码</th><th data-options="field:'card_type',width:50,formatter:formatCardType">卡类型</th></tr></thead><tbody></tbody></table></div><div class="datagrid-pager"></div></div></div>
    </div>
    <script>
	    function onBeforeSearch() {
	    		return $('#search-fm').form('enableValidation').form('validate');
	    }

	    	
	    function formatAmount(value, rowData, rowIndex) {
				return value/100;
			}
			
			function formatCardType(value, rowData, rowIndex) {
				return xTool.formatField(xConfig['card_type'], value);
			}
    </script>


</body></html>